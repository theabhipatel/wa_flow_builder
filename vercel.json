{
    "version": 2,
    "framework": null,
    "installCommand": "npm install && cd server && npm install && cd ../client && npm install",
    "buildCommand": "cd client && npm run build",
    "outputDirectory": "client/dist",
    "rewrites": [
        { "source": "/api/(.*)", "destination": "/api" },
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}
